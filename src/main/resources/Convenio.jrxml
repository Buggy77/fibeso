	<parameter name="FolioConvenio" class="java.lang.String"/>
	<queryString>
		<![CDATA[select cli."Nombre", cli."AppP", cli."AppM",cli."Folioife",conves."DescMnpio",conves."DescEstado",conves."DescPaq", conves."ServiciosPaq",'$ '||conves."TotalConvenio" as "TotalConvenio",conves."total_letra",conves."DatosPago",conves."fec_pag",
 conves."fec_com",conves."FolioConvenio",conves."RfcTitular",
	MAX(CASE WHEN benef.Nro = 1 THEN '1. '||"NombreBenef" ELSE '' END) AS nom_ben1,
	MAX(CASE WHEN benef.Nro = 1 THEN "AppPBenef" ELSE '' END) AS app_ben1,
	MAX(CASE WHEN benef.Nro = 1 THEN "AppMBenef" ELSE '' END) AS apm_ben1,
	MAX(CASE WHEN benef.Nro = 1 THEN '('||"DescPar"||')' ELSE '' END) AS par1,
	MAX(CASE WHEN benef.Nro = 2 THEN '2. '||"NombreBenef" ELSE '' END) AS nom_ben2,
    MAX(CASE WHEN benef.Nro = 2 THEN "AppPBenef" ELSE '' END) AS app_ben2,
	MAX(CASE WHEN benef.Nro = 2 THEN "AppMBenef" ELSE '' END) AS apm_ben2,
	MAX(CASE WHEN benef.Nro = 2 THEN '('||"DescPar"||')'  ELSE '' END) AS par2,
	MAX(CASE WHEN benef.Nro = 3 THEN '3. '||"NombreBenef" ELSE '' END) AS nom_ben3,
    MAX(CASE WHEN benef.Nro = 3 THEN "AppPBenef" ELSE '' END) AS app_ben3,
	MAX(CASE WHEN benef.Nro = 3 THEN "AppMBenef" ELSE '' END) AS apm_ben3,
	MAX(CASE WHEN benef.Nro = 3 THEN '('||"DescPar"||')'  ELSE '' END) AS par3
	from esqfibeso.clientes cli LEFT JOIN
  (select "NombreBenef", "AppPBenef", "AppMBenef", "RfcTitular", par."DescPar",
	ROW_NUMBER() OVER (PARTITION BY "RfcTitular" ORDER BY "RfcTitular") AS Num,
	ROW_NUMBER() OVER (PARTITION BY "RfcTitular", "FolioConvenio"  ORDER BY  "RfcTitular") AS Nro
	from esqfibeso.beneficiarios ben INNER JOIN esqfibeso.parentescos par
			 ON par."IdParentesco" = ben."IdParentesco"
	where
	"FolioConvenio" = $P{FolioConvenio} and  ben."Activo" = true
  ) benef
  ON cli."Rfc" = benef."RfcTitular"
  LEFT JOIN
  (
	  select con."TotalConvenio",
	  	esqfibeso.fu_numero_letras(con."TotalConvenio")as total_letra,
 		"DatosPago",
		to_char("FchPago", 'DD/MM/YYYY') as fec_pag,
		to_char("FchContConve", 'DD/MM/YYYY') as fc_con,
	  			trim(case  when to_char("FchContConve",'d') = '1' then 'Domingo'
  when to_char("FchContConve",'d') = '2' then 'Lunes'
  when to_char("FchContConve",'d') = '3' then 'Martes'
  when to_char("FchContConve",'d') = '4' then 'Miércoles'
  when to_char("FchContConve",'d') = '5' then 'Jueves'
  when to_char("FchContConve",'d') = '6' then 'Viernes'
  when to_char("FchContConve",'d') = '7' then 'Sábado'
 end)||', '|| to_char("FchContConve",'dd')||' de '||trim(case when
	to_char("FchContConve",'mm') = '01' then 'Enero'
  when to_char("FchContConve",'mm') = '02' then 'Febrero'
  when to_char("FchContConve",'mm') = '03' then 'Marzo'
  when to_char("FchContConve",'mm') = '04' then 'Abril'
  when to_char("FchContConve",'mm') = '05' then 'Mayo'
  when to_char("FchContConve",'mm') = '06' then 'Junio'
  when to_char("FchContConve",'mm') = '07' then 'Julio'
  when to_char("FchContConve",'mm') = '08' then 'Agosto'
  when to_char("FchContConve",'mm') = '09' then 'Septiembre'
  when to_char("FchContConve",'mm') = '10' then 'Octubre'
  when to_char("FchContConve",'mm') = '11' then 'Noviembre'
  when to_char("FchContConve",'mm') = '12' then 'Diciembre'
 end)||' de '||trim(to_char("FchContConve",'yyyy')) as fec_com,
	    EXTRACT(YEAR FROM "FchContConve") as anioconv,
		con."FolioConvenio",
		paq."DescPaq",
	  	paq."ServiciosPaq",
	  	est."DescEstado",
		mun."DescMnpio",
		con."IdTipoContratacion",
	    con."RfcTitular",
		ROW_NUMBER() OVER (PARTITION BY "RfcTitular" ORDER BY "FchContConve") AS Num2,
		ROW_NUMBER() OVER (PARTITION BY "RfcTitular", "FolioConvenio"  ORDER BY  "RfcTitular") AS Nro2
		from esqfibeso.convenios con INNER JOIN esqfibeso.paquetes_pfs paq
			 ON con."IdPaqPF" = paq."IdPaqPF"
			 INNER JOIN esqfibeso.cat_mnpios mun
			 ON mun."IdRegMnpioS" = con."IdCiudadContra"
	  	     INNER JOIN esqfibeso.cat_estados est
			 ON est."IdRegEstadoS" = con."IdEstadoContra"
		where "FolioConvenio" = $P{FolioConvenio} and
	  		 con."Activo" = true
	    order by con."FchContConve" asc, con."FchCreReg" asc
  ) conves
  ON cli."Rfc" = conves."RfcTitular"
where "FolioConvenio" = $P{FolioConvenio}
GROUP BY cli."Nombre", cli."AppP", cli."AppM",cli."Folioife",conves."DescMnpio",conves."DescEstado",conves."DescPaq",
 conves."ServiciosPaq",conves."TotalConvenio",conves."total_letra",conves."DatosPago",conves."fec_pag",
 conves."fec_com",conves."FolioConvenio",conves."RfcTitular";]]>
	</queryString>
